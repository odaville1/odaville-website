{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "frontend",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/blog",
      "dest": "/api/blog/index.js"
    },
    {
      "src": "/api/gallery",
      "dest": "/api/gallery/index.js"
    },
    {
      "src": "/api/products",
      "dest": "/api/products/index.js"
    },
    {
      "src": "/api/diagnostic",
      "dest": "/api/diagnostic.js"
    },
    {
      "src": "/brochures/(.*)",
      "dest": "/brochures/$1"
    },
    {
      "src": "/admin/(.*).html",
      "dest": "/admin/$1.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/(.*)\\.html",
      "dest": "/$1.html"
    },
    {
      "src": "/(.*)\\.(js|css|jpg|jpeg|png|gif|svg|ico|webp|ttf|woff|woff2|otf)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}